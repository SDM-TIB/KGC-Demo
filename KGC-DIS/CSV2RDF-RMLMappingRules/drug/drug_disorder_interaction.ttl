@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix covid-19: <http://research.tib.eu/covid-19/vocab/> .

<#DrugDisorderInteraction>
a rr:TriplesMap;
	rml:logicalSource [ rml:source "/data/drug/drugs_indications.csv";
	rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://research.tib.eu/covid-19/entity/{DrugId}_isPrescribedFor_{indication_Id}";
		rr:class covid-19:DrugDisorderInteraction
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasDrug;
		rr:objectMap [ 
			rr:template "http://research.tib.eu/covid-19/entity/{DrugId}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasIndication;
		rr:objectMap [ 
			rr:template "http://covid-19.eu/entity/{indication_name}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:hasPrimarySource;
		rr:objectMap [
			rr:template "http://research.tib.eu/covid-19/SourceName/DrugBank"
		]
	].	

