@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix covid-19: <http://research.tib.eu/covid-19/vocab/> .

<Disorder>
a rr:TriplesMap;
	rml:logicalSource [ rml:source "/data/drug/indication_CUI.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://research.tib.eu/covid-19/entity/{name}";
		rr:class covid-19:Disorder
	];
			
	rr:predicateObjectMap [
		rr:predicate covid-19:hasCUIAnnotation;
		rr:objectMap [
			rr:template "http://research.tib.eu/covid-19/entity/{ID}";
		]
	].


